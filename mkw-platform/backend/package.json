{
  "name": "mkw-platform-backend",
  "version": "1.0.0",
  "description": "MKW Advisors Enterprise Platform - Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "db:setup": "npm run migrate && npm run seed",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "echo 'No build step required'",
    "health": "curl -f http://localhost:5000/health || exit 1"
  },
  "keywords": [
    "crm",
    "enterprise",
    "business-management",
    "nodejs",
    "express",
    "postgresql"
